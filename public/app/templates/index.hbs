<div id="main">
    <div class="buttons">
        <div class="left half"><button id="create_note">Create new Note</button></div>
        <div class="left half"><select><option>Black White-Style</option></select></div>
    </div>
    <div class="clear"></div>
    <div class="order">
        <div class="left"><button id="order_finish">By finished Date</button></div>
        <div class="left"><button id="order_created">By created Date</button></div>
        <div class="left"><button id="order_importance">By Importance</button></div>
    </div>
    <div class="clear"></div>

    <div class="clear"></div>
    <div class="item">
    </div>
    <ul class="items">
        {{#each notes}}
            <li>
                <div class="title">
                    <div class="left"><strong>{{this.title}}</strong> </div>
                    <div class="left"> <em>{{this.description}}</em> </div>
                    <div class="left"> {{this.rate}}</div>
                </div>
                <button class="btn-row-listener" data-id="{{this.id}}">Edit</button>
            </li>
        {{/each}}
    </ul>

</div>


<a href="#create_note">MAKE NEW ON</a>
